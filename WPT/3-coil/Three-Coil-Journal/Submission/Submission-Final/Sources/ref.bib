journal = {IEEE Trans. Power Electron.},
keywords = {Electric vehicles (EVs),inductive charging,resonance,three-phase electric power,tuned circuits,wireless charging,wireless power transfer (WPT)},
mendeley-groups = {WPT/Emerging Topic TPEL},
number = {5},
pages = {4500--4517},
publisher = {IEEE},
title = {{A 50-kW Three-Phase Wireless Power Transfer System Using Bipolar Windings and Series Resonant Networks for Rotating Magnetic Fields}},
