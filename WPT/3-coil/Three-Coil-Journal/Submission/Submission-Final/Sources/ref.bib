  @article{Aditya2019,
abstract = {Resonant inductive power transfer (RIPT) is gaining in popularity for wireless charging applications of future electric transportation. A fundamental impediment to the efficient operation of an RIPT system is the existence of bifurcation phenomenon in a doubly tuned circuit. Bifurcation can be avoided by either selecting complicated control strategies or by proper design strategies. Many control strategies have been presented in the literature for avoiding bifurcation. However, systematic design strategies for avoiding bifurcation are still unknown. This paper presents a simplified, and easy to follow set of design guidelines for series-series compensated RIPT (SS-RIPT) systems. The design guidelines avoid bifurcation by calculating the parameters for a given load profile in a systematic and easy to follow approach. Following the design guidelines, a 3.6-kW setup has been fabricated as an example and verified using a finite element analysis as well as experimental testing. Selection of a resonant frequency and output voltage for 3.6 kW was influenced by the availability of testing equipment available in the lab. However, the design guidelines are generalized, and by following it, an SS-RIPT system of any frequency and output voltage rating, applicable for EV charging, can be designed.},
author = {Aditya, Kunwar and Williamson, Sheldon S.},
doi = {10.1109/TIE.2018.2851953},
file = {:C$\backslash$:/Users/user/Desktop/08408694.pdf:pdf},
issn = {02780046},
journal = {IEEE Trans. Ind. Electron.},
keywords = {Battery chargers,bifurcation,electric vehicles,energy storage,inductive power transfer},
mendeley-groups = {WPT},
number = {5},
pages = {3973--3982},
publisher = {IEEE},
title = {{Design guidelines to avoid bifurcation in a series-series compensated inductive power transfer system}},
volume = {66},
year = {2019}
}


